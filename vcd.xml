<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="Azure IoT Robot">
    <CommandPrefix>Robot</CommandPrefix>
    <Example>Robot wave</Example>
    <Command Name="robot">
      <Example>Robot {message}</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">robot {streamSubject}</ListenFor>
      <Feedback>sending command {streamSubject} to robot</Feedback>
      <Navigate Target="/default.aspx"/>
    </Command>
    <PhraseTopic Label="streamSubject" Scenario="Dictation"></PhraseTopic>
  </CommandSet>
</VoiceCommands>
